{
  "name": "next-mdx-static-blog-kit",
  "version": "1.0.0",
  "description": "A starter kit for creating a static website blog using Next.js and MDX",
  "engines": {
    "node": "10"
  },
  "main": "firebaseFunctions.js",
  "scripts": {
    "dev": "next src/",
    "dev:bak": "NODE_ENV=development next",
    "build": "next build",
    "build:bak": "NODE_ENV=production next build",
    "build-dev": "NODE_ENV=development next build",
    "start": "NODE_ENV=production next start",
    "clean": "rm -rf .next ; npm ci ; NODE_ENV=development npm run dev",
    "now-build": "next build && next export -o dist",
    "deploy": "firebase deploy --only functions,hosting",
    "test": "jest"
  },
  "keywords": [
    "next.js",
    "react.js",
    "blog",
    "mdx"
  ],
  "author": "Loren Stewart",
  "license": "ISC",
  "bin": {
    "blog": "cli.js"
  },
  "browserslist": [
    "last 2 version"
  ],
  "dependencies": {
    "@babel/runtime-corejs2": "^7.10.5",
    "@mdx-js/loader": "^1.6.14",
    "@mdx-js/mdx": "^0.15.7",
    "@next/mdx": "^8.1.0",
    "@zeit/next-mdx": "1.1.0",
    "babel-plugin-preval": "^4.0.0",
    "chalk": "^3.0.0",
    "firebase-admin": "^8.9.0",
    "firebase-functions": "^3.8.0",
    "js-yaml": "^3.13.1",
    "js-yaml-loader": "^1.2.2",
    "next": "^9.4.0",
    "next-fonts": "^0.19.0",
    "next-seo": "^3.4.0",
    "react": "^16.12.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-syntax-highlighter": "^9.0.1",
    "remark-emoji": "^2.0.2",
    "remark-images": "^1.0.0",
    "scrollprogress": "^3.0.2",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.0.0",
    "jest": "^24.9.0",
    "next-optimized-images": "^2.5.4",
    "svg-sprite-loader": "^4.1.6",
    "webp-loader": "^0.6.0"
  }
}
